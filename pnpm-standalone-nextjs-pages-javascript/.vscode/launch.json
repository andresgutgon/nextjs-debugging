{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "attach",
      "name": "attach-working (pwa-node)",
      "processId": "${command:pickProcess}",
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Next.js: debug server-side",
      "type": "pwa-node",
      "request": "launch",
      "command": "NODE_OPTIONS='--inspect' pnpm dev"
    },
    {
      "name": "Next.js: (vscode-working-node-terminal)",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm dev"
    }
  ]
}

